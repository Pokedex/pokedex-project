<h2>My team:</h2>

<div id="pokemon-display">
  {{#each result}}
     <a href="/pokemon/{{name}}">
      <img style="width: 200px; display: inline-block;" src="https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/other/official-artwork/{{number}}.png" alt="{{name}}">
      <p style="display: inline;">{{name}}</p>
     </a>
     <br>
  {{/each}}
</div>

