<h2>My team:</h2>

<div id="pokemon-display">
  {{#each team}}
     <a href="/pokemon/{{this}}">
      <img style="width: 200px; display: inline-block;" src="/images/{{this}}.png" alt="{{this}}">
      <p style="display: inline;">{{this}}</p>
     </a>
     <br>
  {{/each}}
</div>

<script src="/javascripts/pokemonScript.js"></script>